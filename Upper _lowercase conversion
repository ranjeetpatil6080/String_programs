package String;

public class String_UpperLowerCase {

	public static void main(String[] args) {
		
		String name[]= {"Tata","Mahindra","Jaguar","Bentley","Bajaj"};
		
    for(int i=0;i<name.length;i++)
		  {
			System.out.println(name[i].toLowerCase());      //coverts all char into lowercase
		  }
		for(int i=0;i<name.length;i++)
		  {
			System.out.println(name[i].toUpperCase());       //coverts all char into an uppercase
		  }

	}

}
